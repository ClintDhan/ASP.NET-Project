@{
    Layout = "~/Views/Shared/UserLayout.cshtml";
}

@section Title {
User Notification
}

<div class="header-color"></div>
<div class="card-container">
<div class="dashboard-table-container">
    <h2>Notifications</h2>

    <table class="dashboard-table">
        <thead>
            <tr>

                <th>Name</th>
                <th>Message</th>
                <th>Date</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>
</div>

@section Scripts {
    <script>
        document.getElementById("UserNotification").classList.add("active");
    </script>
}
